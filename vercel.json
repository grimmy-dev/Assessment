{
  "version": 2,
  "functions": {
    "src/api/main.py": {
      "runtime": "python3.9"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/api/main.py"
    },
    {
      "src": "/docs",
      "dest": "src/api/main.py"
    },
    {
      "src": "/openapi.json",
      "dest": "src/api/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
